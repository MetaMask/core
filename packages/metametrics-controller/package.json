{
  "name": "@metamask/metametrics-controller",
  "version": "1.0.0",
  "description": "Metadata tracking for MetaMask.",
  "keywords": [
    "MetaMask",
    "Ethereum"
  ],
  "homepage": "https://github.com/MetaMask/core/tree/main/packages/metametrics-controller#readme",
  "bugs": {
    "url": "https://github.com/MetaMask/core/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/core.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsup --config ../../tsup.config.ts --tsconfig ./tsconfig.build.json --clean",
    "build:docs": "typedoc",
    "changelog:update": "../../scripts/update-changelog.sh @metamask/metametrics-controller",
    "changelog:validate": "../../scripts/validate-changelog.sh @metamask/metametrics-controller",
    "publish:preview": "yarn npm publish --tag preview",
    "test": "jest --reporters=jest-silent-reporter",
    "test:clean": "jest --clearCache",
    "test:verbose": "jest --verbose",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@ethereumjs/util": "^8.1.0",
    "@metamask/base-controller": "^5.0.2",
    "@metamask/controller-utils": "^9.1.0",
    "@metamask/name-controller": "^6.0.1",
    "@metamask/obs-store": "^9.0.0",
    "assert": "^2.1.0",
    "ethereumjs-util": "^7.1.3",
    "lodash": "^4.17.21",
    "sinon": "^9.2.4",
    "uuid": "^8.3.2",
    "webextension-polyfill": "^0.12.0"
  },
  "devDependencies": {
    "@metamask/auto-changelog": "^3.4.4",
    "@types/jest": "^27.4.1",
    "@types/readable-stream": "^2.3.0",
    "@types/webextension-polyfill": "^0.10.7",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "typedoc": "^0.24.8",
    "typedoc-plugin-missing-exports": "^2.0.0",
    "typescript": "~4.9.5",
    "webextension-polyfill-ts": "^0.26.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}